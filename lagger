import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
int rows=sc.nextInt();
System.out.println(" ");
int i,j,k,l=1;
for(i=1; i<=rows; i++ ){

 for(j=1; j<=rows-i; j++){

 }
 for(k=1; k<=i; k++,l++){
 System.out.print(l+" ");
 }
  System.out.println(" ");

 }
 
}
}

Output:

 
1  
2 3  
4 5 6  
